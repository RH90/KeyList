<Window x:Class="KeyList.AddComputer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KeyList"
        mc:Ignorable="d"
         Title="Lägg till" Height="451.169" Width="319.61">
    <Grid Background="Black">
        <GroupBox Foreground="White" Header="Dator">
            <StackPanel Margin="0,0,0,50">
                <TextBlock Text="Brand"></TextBlock>
                <TextBox x:Name="tbBrand"></TextBox>
                <TextBlock Text="Model"></TextBlock>
                <TextBox  x:Name="tbModel"></TextBox>
                <TextBlock Text="Serielnumber"></TextBlock>
                <TextBox  x:Name="tbSerielnumber"></TextBox>
                <TextBlock Text="Smartwater"></TextBlock>
                <TextBox x:Name="tbSmartwater"></TextBox>
            </StackPanel>
        </GroupBox>
        <Button x:Name="bOK" Content="OK" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="28,381,0,0" Click="bOK_Click" />
        <Button x:Name="bCancel" Content="Cancel" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="185,381,0,0" Click="bCancel_Click"/>
    </Grid>
</Window>
